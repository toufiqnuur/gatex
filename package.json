{
  "name": "ghnote",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier --ignore-path .gitignore --write .",
    "prepare": "husky install"
  },
  "dependencies": {
    "github-markdown-css": "^5.1.0",
    "html-react-parser": "^1.4.9",
    "marked": "^4.0.12",
    "next": "12.1.0",
    "react": "17.0.2",
    "react-dom": "17.0.2"
  },
  "devDependencies": {
    "@types/node": "17.0.23",
    "@types/react": "17.0.43",
    "eslint": "8.11.0",
    "eslint-config-next": "12.1.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.7",
    "prettier": "^2.6.1",
    "typescript": "4.6.3"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "eslint --cache --fix"
    ],
    "*.{html,css,json,js,jsx,ts,tsx}": [
      "prettier --write"
    ]
  }
}
